<configuration xmlns="http://byblo.org"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://byblo.org defaults.xsd">

    <measure>lin</measure>
    <charset>UTF-8</charset>
    <locale>en-GB</locale>

    <fileFormats>

        <fileFormat>
            <name>instances</name>
            <skipIndex>
                <column>false</column>
                <column>false</column>
            </skipIndex>
            <compact>true</compact>
        </fileFormat>

        <fileFormat>
            <name>entries</name>
            <skipIndex>
                <column>true</column>
            </skipIndex>
        </fileFormat>

        <fileFormat>
            <name>features</name>
            <skipIndex>
                <column>true</column>
            </skipIndex>
        </fileFormat>

        <fileFormat>
            <name>events</name>
            <skipIndex>
                <column>true</column>
                <column>true</column>
            </skipIndex>
            <compact>true</compact>
        </fileFormat>

        <fileFormat>
            <name>sims</name>
            <skipIndex>
                <column>false</column>
                <column>false</column>
            </skipIndex>
            <compact>true</compact>
        </fileFormat>

        <fileFormat>
            <name>neighbours</name>
            <skipIndex>
                <column>false</column>
                <column>false</column>
            </skipIndex>
            <compact>true</compact>
        </fileFormat>

    </fileFormats>


    <measures>

        <measure>
            <name>confusion</name>
            <description>Confusion Probability</description>
            <aliases>
                <alias>confusion</alias>
                <alias>conf</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.Confusion</class>
        </measure>

        <measure>
            <name>cosine</name>
            <description>Consine Similarity</description>
            <aliases>
                <alias>cosine</alias>
                <alias>cos</alias>
                <alias>pearson</alias>
                <alias>product-moment</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.Cosine</class>
        </measure>

        <measure>
            <name>dice</name>
            <description>Dice's Co-efficient</description>
            <aliases>
                <alias>dice</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.Dice</class>
        </measure>

        <measure>
            <name>dotproduct</name>
            <description>Inner-Product: Proximity measure calculating
                similarity as the dot product of the vectors. For unit vectors
                this is equivalent to Cosine.
            </description>
            <aliases>
                <alias>dot_product</alias>
                <alias>inner_product</alias>
                <alias>dp</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.DotProduct</class>
        </measure>

        <measure>
            <name>hindle</name>
            <description>Hindle</description>
            <aliases>
                <alias>hindle</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.Hindle</class>
        </measure>


        <measure>
            <name>jaccard</name>
            <description>Jaccard Index</description>
            <aliases>
                <alias>jaccard</alias>
                <alias>jacc</alias>
                <alias>j</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.Jaccard</class>
        </measure>

        <measure>
            <name>js</name>
            <description>Jensen-Shannon Divergence</description>
            <aliases>
                <alias>jenson</alias>
                <alias>js</alias>
                <alias>jensen</alias>
                <alias>jensen-shannon</alias>
                <alias>jenson-shannon</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.JensenShannonDivergence</class>
        </measure>

        <measure>
            <name>kendaltau</name>
            <description>Kendall's Tau Rank Correlation Coefficient</description>
            <aliases>
                <alias>kendall</alias>
                <alias>tau</alias>
                <alias>kendaltau</alias>
                <alias>kendalstau</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.KendallsTau</class>
        </measure>

        <measure>
            <name>kl</name>
            <description>Kullbackâ€“Leibler Divergence</description>
            <aliases>
                <alias>kl-divergence</alias>
                <alias>kld</alias>
                <alias>kl</alias>
                <alias>kullback</alias>
                <alias>leibler</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.KullbackLeiblerDivergence</class>
        </measure>


        <measure>
            <name>lambda</name>
            <description>Lambda Divergence</description>
            <aliases>
                <alias>lambda-divergence</alias>
                <alias>lambda</alias>
                <alias>ld</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.LambdaDivergence</class>
            <parameters>
                <parameter>
                    <name>lambda</name>
                    <type>double</type>
                    <defaultValue>0.5</defaultValue>
                    <required>false</required>
                </parameter>
            </parameters>
        </measure>


        <measure>
            <name>lin</name>
            <description>Lin</description>
            <aliases>
                <alias>lin</alias>
                <alias>lin98</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.Lin</class>

        </measure>

        <measure>
            <name>lee</name>
            <description>Lee Alpha-Skew Divergence</description>
            <aliases>
                <alias>lee</alias>
                <alias>alpha-skew</alias>
                <alias>alphaskew</alias>
                <alias>skew_divergence</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.LeeSkewDivergence</class>
            <parameters>
                <parameter>
                    <name>alpha</name>
                    <type>double</type>
                    <defaultValue>0.99</defaultValue>
                    <required>false</required>
                </parameter>
            </parameters>
        </measure>

        <measure>
            <name>lp</name>
            <description>Lp-Space Distance</description>
            <aliases>
                <alias>lp</alias>
                <alias>lpspace</alias>
                <alias>minkowski</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.LpSpaceDistance</class>
            <parameters>
                <parameter>
                    <name>power</name>
                    <type>double</type>
                    <defaultValue>2</defaultValue>
                    <required>false</required>
                </parameter>
            </parameters>
        </measure>

        <measure>
            <name>overlap</name>
            <description>Overlap</description>
            <aliases>
                <alias>overlap</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.Overlap</class>
        </measure>

        <measure>
            <name>precision</name>
            <description>Precision</description>
            <aliases>
                <alias>precision</alias>
                <alias>prec</alias>
                <alias>prc</alias>
                <alias>p</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.Precision</class>
        </measure>

        <measure>
            <name>recall</name>
            <description>Recall</description>
            <aliases>
                <alias>recall</alias>
                <alias>rcl</alias>
                <alias>rec</alias>
                <alias>r</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.Recall</class>
        </measure>


        <measure>
            <name>weeds</name>
            <description>Weed's Mutual Information Measure</description>
            <aliases>
                <alias>crmi</alias>
                <alias>cr_mi</alias>
                <alias>cri</alias>
                <alias>weeds</alias>
            </aliases>
            <class>uk.ac.susx.mlcl.byblo.measures.impl.Weeds</class>
            <parameters>
                <parameter>
                    <name>beta</name>
                    <type>double</type>
                    <defaultValue>0.5</defaultValue>
                    <required>false</required>
                </parameter>
                <parameter>
                    <name>gamma</name>
                    <type>double</type>
                    <defaultValue>0.5</defaultValue>
                    <required>false</required>
                </parameter>
            </parameters>

        </measure>

    </measures>


    <weightings>
        <weighting>
            <name>chi2</name>
            <description>Chi Squared</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.ChiSquared</class>
            <aliases>
                <alias>chi2</alias>
            </aliases>
        </weighting>

        <weighting>
            <name>constant</name>
            <description>Constant</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.Constant</class>
            <aliases>
                <alias>const</alias>
            </aliases>
            <parameters>
                <parameter>
                    <name>constant</name>
                    <type>double</type>
                    <defaultValue>1</defaultValue>
                    <required>false</required>
                </parameter>
            </parameters>
        </weighting>

        <weighting>
            <name>dice</name>
            <description>Dice</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.DiceWeighting</class>
        </weighting>

        <weighting>
            <name>unit</name>
            <description>Unit Vector</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.L2UnitVector</class>
        </weighting>


        <weighting>
            <name>llr</name>
            <description>Log-Likelihood-Ratio</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.LLR</class>
        </weighting>


        <weighting>
            <name>likelihood</name>
            <description>Likelihood</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.Likelihood</class>
        </weighting>

        <weighting>
            <name>logprod</name>
            <description>Log-Product</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.LogProduct</class>
        </weighting>

        <weighting>
            <name>pmi</name>
            <description>Pointwise-Mutual-Information</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.PMI</class>
        </weighting>

        <weighting>
            <name>npmi</name>
            <description>Normalised Pointwise-Mutual-Information</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.NormalisedPMI</class>
        </weighting>


        <weighting>
            <name>wpmi</name>
            <description>Weighted Pointwise-Mutual-Information</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.WeightedPMI</class>
        </weighting>


        <weighting>
            <name>ppmi</name>
            <description>Positive Pointwise-Mutual-Information</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.PositivePMI</class>
        </weighting>


        <weighting>
            <name>power</name>
            <description>Power</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.Power</class>
            <parameters>
                <parameter>
                    <name>power</name>
                    <type>double</type>
                    <defaultValue>2</defaultValue>
                    <required>false</required>
                </parameter>
            </parameters>
        </weighting>

        <weighting>
            <name>step</name>
            <description>Step</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.Step</class>
            <parameters>
                <parameter>
                    <name>boundary</name>
                    <type>double</type>
                    <defaultValue>0</defaultValue>
                    <required>false</required>
                </parameter>
            </parameters>
        </weighting>


        <weighting>
            <name>ttest</name>
            <description>T-Test</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.TTest</class>
        </weighting>


        <weighting>
            <name>geomean</name>
            <description>Geometric Mean</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.GeoMean</class>
        </weighting>


        <weighting>
            <name>rank</name>
            <description>Rank</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.Rank</class>
        </weighting>

        <weighting>
            <name>sqpmi</name>
            <description>Squared Pointwise Mutual Information</description>
            <class>uk.ac.susx.mlcl.byblo.weightings.impl.SquaredPMI</class>
        </weighting>


    </weightings>


</configuration>

